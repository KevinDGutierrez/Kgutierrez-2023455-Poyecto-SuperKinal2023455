<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kevingutierrez.controller.LoginController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Login.png" />
         </image></ImageView>
      <TextField fx:id="tfUser" layoutX="77.0" layoutY="314.0" prefHeight="43.0" prefWidth="262.0" />
      <PasswordField fx:id="tfPassword" layoutX="77.0" layoutY="412.0" prefHeight="43.0" prefWidth="262.0" />
      <Button fx:id="btnIniciar" layoutX="179.0" layoutY="537.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="54.0" prefWidth="125.0" text="Iniciar Sesion" />
      <Button fx:id="btnRegistrar" layoutX="179.0" layoutY="628.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="54.0" prefWidth="125.0" text="Registrar Usuario" />
   </children>
</AnchorPane>
