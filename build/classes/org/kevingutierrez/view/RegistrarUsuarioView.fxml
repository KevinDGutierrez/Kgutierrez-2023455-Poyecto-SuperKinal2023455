      <TextField fx:id="tfUser" layoutX="78.0" layoutY="211.0" prefHeight="38.0" prefWidth="234.0" />
      <TextField fx:id="tfPassword" layoutX="78.0" layoutY="307.0" prefHeight="38.0" prefWidth="226.0" />
      <ComboBox fx:id="cmbEmpleado" layoutX="78.0" layoutY="419.0" prefHeight="38.0" prefWidth="234.0" />
