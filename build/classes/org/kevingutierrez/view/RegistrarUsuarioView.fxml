      <Button fx:id="btnEmpleado" layoutX="103.0" layoutY="579.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="56.0" prefWidth="122.0" text="Agregar Empleado" />
      <Button fx:id="btnRegistrar" layoutX="251.0" layoutY="579.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="56.0" prefWidth="122.0" text="Registrar" />
